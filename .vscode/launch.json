{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Alembic autogen",
			"type": "debugpy",
			"request": "launch",
			"module": "alembic",
			"args": [
				"-x",
				"schema=index",
				"revision",
				"--autogenerate",
				"-m",
				"test"
			],
			"cwd": "${workspaceFolder}/projects/genomicsdb-schema-manager"
		},
		/*{
            "name": "Sphinx Autobuild",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/sphinx-build",
            "args": [
                "docs",
                "docs/_build/html"
            ],
            "justMyCode": false
        }, */
		{
			"name": "FILER - dev",
			"type": "debugpy",
			"request": "launch",
			"module": "uvicorn",
			"args": [
				"niagads.filer.api.core:app",
				"--reload",
				"--env-file",
				"${workspaceFolder}/../dev.env"
				//"--no-reload",
			],
			"jinja": true,
			"justMyCode": false
		},
		{
			"name": "Genomics - dev",
			"type": "debugpy",
			"request": "launch",
			"module": "uvicorn",
			"args": [
				"niagads.genomicsdb.api.core:app",
				"--reload",
				"--port",
				"8005",
				"--env-file",
				"${workspaceFolder}/../genomics.env"
				//"--no-reload",
			],
			"jinja": true,
			"justMyCode": false
		},
		{
			"name": "NIAGADS - dev",
			"type": "debugpy",
			"request": "launch",
			"module": "uvicorn",
			"args": [
				"niagads.open_access.api.core:app",
				"--reload",
				//"--port", "8001",
				"--env-file",
				"${workspaceFolder}/../niagads.env"
				//"--no-reload",
			],
			"jinja": true,
			"justMyCode": false
		}
		/*{
            "name": "FAST-API - plus dependencies",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "api.main:app",
                "--reload"
                //"--no-reload",
            ],
            "jinja": true,
            "justMyCode": false
        }*/
	]
}
